<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <column name="id" display="none" order="100">
            <widget type="text" fieldname="idproducto" readonly="true"/>
        </column>
        <column name="woo_id" display="none" order="101">
            <widget type="number" fieldname="woo_id" readonly="true"/>
        </column>
        <column name="name" display="" order="110">
            <widget type="text" fieldname="woo_product_name"/>
        </column>
        <column name="price" display="left" order="115">
            <widget type="number" fieldname="woo_price"/>
        </column>
        <column name="status" display="left" order="120">
            <widget type="text" fieldname="woo_status"/>
        </column>
        <column name="Vinculado" display="left" order="125">
            <widget type="text" fieldname="vinculado">
                <option color="success">Si</option>
                <option color="danger">No</option>
            </widget>
        </column>
        <column name="permalink" display="left" order="130">
            <widget type="textarea" fieldname="woo_permalink"/>
        </column>
        <column name="creation-date" display="left" order="140">
            <widget type="datetime" fieldname="creation_date"/>
        </column>
    </columns>

    <rows>
    <row type="status" fieldname="vinculado">
        <option color="light" title="Vinculado">Si</option>
        <option color="warning" title="No Vinculado">No</option>
    </row>
</rows>
</view>
