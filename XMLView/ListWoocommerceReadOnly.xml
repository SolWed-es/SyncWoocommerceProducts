<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <column name="id" display="none" order="100">
            <widget type="text" fieldname="id" readonly="true"/>
        </column>
        <column name="catalog_visibility" display="left" order="110">
            <widget type="text" fieldname="catalog_visibility" maxlength="20"/>
        </column>
        <column name="categories" display="left" order="120">
            <widget type="textarea" fieldname="categories"/>
        </column>
        <column name="images" display="left" order="130">
            <widget type="textarea" fieldname="images"/>
        </column>
        <column name="name" display="" order="140">
            <widget type="text" fieldname="name" maxlength="100"/>
        </column>
        <column name="name" display="left" order="150">
            <widget type="text" fieldname="name" maxlength="200"/>
        </column>
        <column name="permalink" display="left" order="160">
            <widget type="textarea" fieldname="permalink"/>
        </column>
        <column name="price" display="left" order="170">
            <widget type="number" fieldname="price" max="0" min="0" step="0"/>
        </column>
        <column name="sku" display="left" order="180">
            <widget type="text" fieldname="sku" maxlength="100"/>
        </column>
        <column name="status" display="left" order="190">
            <widget type="text" fieldname="status" maxlength="20"/>
        </column>
        <column name="woo_id" display="left" order="200">
            <widget type="number" fieldname="woo_id" max="0" min="0" step="0"/>
        </column>
        <column name="creation-date" display="none" order="210">
            <widget type="datetime" fieldname="creation_date" required="true"/>
        </column>
        <column name="user" order="220">
            <widget type="select" fieldname="nick">
                <values source="users" fieldcode="nick" fieldtile="nick"/>
            </widget>
        </column>
        <column name="last-update" display="none" order="230">
            <widget type="datetime" fieldname="last_update"/>
        </column>
        <column name="last-user" order="240">
            <widget type="select" fieldname="last_nick">
                <values source="users" fieldcode="nick" fieldtile="nick"/>
            </widget>
        </column>
    </columns>
</view>
